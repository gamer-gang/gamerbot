{
  "name": "gamerbot",
  "version": "2.5.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "workspaces": [
    "types"
  ],
  "scripts": {
    "esbuild": "esbuild $(echo $(cat .esbuild)) --banner:js=\"globalThis.GAMERBOT_VERSION='$(echo $(node ./scripts/version.js))'\"",
    "_build": "yarn esbuild --outdir=dist src/index.ts src/docgen.ts",
    "build": "yarn _build --minify --sourcemap",
    "watch": "yarn _build --watch",
    "docgen": "node -r source-map-support/register ./dist/docgen.js",
    "types": "tsc && sed -i -e 's|\"src|\"gamerbot/src|g' -e 's|\"scripts|\"gamerbot/scripts|g' ./types/gamerbot.d.ts",
    "precommit": "yarn build && yarn docgen && yarn types",
    "postinstall": "patch-package",
    "dev": "NODE_ENV=development nodemon -r source-map-support/register ./dist/index.js",
    "prod": "node ./dist/index.js",
    "scripts:build": "yarn esbuild --outdir=scripts/build ./scripts/*.ts",
    "scripts:watch": "yarn build-scripts --watch",
    "docker:all": "docker-compose up --no-log-prefix --attach app",
    "docker:db": "docker-compose up db --no-log-prefix",
    "docker:build": "docker build . --tag ghcr.io/gamer-gang/gamerbot:latest",
    "docker:tag": "docker tag ghcr.io/gamer-gang/gamerbot:latest ghcr.io/gamer-gang/gamerbot:$(echo $(node ./scripts/version.js))",
    "docker:push": "docker push -a ghcr.io/gamer-gang/gamerbot",
    "git:tag": "git tag v$(echo $(node ./scripts/version.js)) -m \"v$(echo $(node ./scripts/version.js))\""
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "standard": {
    "engine": "ts-standard"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "resolutions": {
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "@maman/patch-package": "^6.4.7-p1",
    "@napi-rs/canvas": "^0.1.30",
    "@prisma/client": "4.6.1",
    "@vvo/tzdb": "^6.76.0",
    "@wiisportsresorts/cowsay": "^0.2.0",
    "bad-words": "^3.0.4",
    "common-tags": "^1.8.2",
    "didyoumean": "^1.2.2",
    "discord.js": "^14.6.0",
    "emoji-regex": "^10.2.1",
    "he": "^1.2.0",
    "hypixel-cache": "wiisportsresort/hypixel-cache#f15a3bfc24dd05ae58534d5feff2eddd5ec9e98b",
    "hypixel-types": "wiisportsresort/hypixel-types#bc679c2ac1c436a2b6131819e8ff22df7406b287",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "log4js": "^6.6.1",
    "luxon": "^3.2.1",
    "mathjax-full": "3.2.1",
    "mathjs": "^11.3.3",
    "nodemon": "^2.0.19",
    "object-hash": "^3.0.0",
    "piston-client": "^1.0.2",
    "postinstall-postinstall": "^2.1.0",
    "regenerator-runtime": "^0.13.10",
    "sharp": "^0.31.2",
    "source-map-support": "^0.5.21",
    "string-length": "^5.0.1",
    "tzdata": "^1.0.35",
    "unicode": "^14.0.0"
  },
  "devDependencies": {
    "@types/bad-words": "^3.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/didyoumean": "^1.2.0",
    "@types/dts-bundle": "^0.0.32",
    "@types/he": "^1.1.2",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.188",
    "@types/luxon": "^3.1.0",
    "@types/object-hash": "^2.2.1",
    "@types/prettier": "^2.7.1",
    "@types/sharp": "^0.31.0",
    "@types/source-map-support": "^0.5.6",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "discord-api-types": "0.37.17",
    "dotenv": "^16.0.3",
    "esbuild": "^0.15.13",
    "eslint": "latest",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.5.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.7.1",
    "prisma": "4.6.1",
    "typescript": "4.9.3"
  }
}
