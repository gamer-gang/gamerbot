{
  "name": "gamerbot",
  "version": "2.10.2",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@3.2.3",
  "private": true,
  "workspaces": [
    "types"
  ],
  "scripts": {
    "dev": "NODE_ENV=development nodemon -r source-map-support/register ./dist/index.js",
    "build": "./scripts/build.js --outdir=./dist src/index.ts src/docgen.ts --minify --production",
    "watch": "./scripts/build.js --outdir=./dist src/index.ts src/docgen.ts --watch",
    "docgen": "node -r source-map-support/register ./dist/docgen.js",
    "types": "tsc && sed -i -e 's|\"src|\"gamerbot/src|g' -e 's|\"scripts|\"gamerbot/scripts|g' ./types/gamerbot.d.ts",
    "db": "docker-compose up db --no-log-prefix",
    "git:tag": "git tag v$(echo $(./scripts/version.js --version-only)) -m \"v$(echo $(./scripts/version.js --version-only))\"",
    "publish": "./scripts/publish.js",
    "precommit": "yarn build && concurrently yarn:docgen yarn:types",
    "release": "yarn git:tag && yarn publish && git push && git push --tags"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "standard": {
    "engine": "ts-standard"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "resolutions": {
    "node-fetch": "^2.6.7",
    "json5": "^1.0.2",
    "http-cache-semantics": "^4.1.1"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.39",
    "@prisma/client": "4.12.0",
    "@sentry/node": "^7.51.2",
    "@sentry/profiling-node": "^0.3.0",
    "@t3-oss/env-core": "^0.3.1",
    "@vvo/tzdb": "^6.105.0",
    "@wiisportsresorts/cowsay": "^0.3.2",
    "bad-words": "^3.0.4",
    "cheerio": "^1.0.0-rc.12",
    "common-tags": "^1.8.2",
    "didyoumean": "^1.2.2",
    "discord.js": "^14.8.0",
    "emoji-regex": "^10.2.1",
    "he": "^1.2.0",
    "hypixel-cache": "wiisportsresort/hypixel-cache#f15a3bfc24dd05ae58534d5feff2eddd5ec9e98b",
    "hypixel-types": "wiisportsresort/hypixel-types#bc679c2ac1c436a2b6131819e8ff22df7406b287",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "luxon": "^3.3.0",
    "mathjax-full": "3.2.2",
    "mathjs": "^11.7.0",
    "nodemon": "^2.0.22",
    "object-hash": "^3.0.0",
    "piston-client": "^1.0.2",
    "regenerator-runtime": "^0.13.11",
    "sharp": "^0.32.0",
    "source-map-support": "^0.5.21",
    "string-length": "^5.0.1",
    "tzdata": "^1.0.37",
    "unicode": "^14.0.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@sentry/esbuild-plugin": "^0.7.2",
    "@types/bad-words": "^3.0.1",
    "@types/common-tags": "^1.8.1",
    "@types/didyoumean": "^1.2.0",
    "@types/dts-bundle": "^0.0.32",
    "@types/he": "^1.2.0",
    "@types/js-yaml": "^4.0.5",
    "@types/lodash": "^4.14.192",
    "@types/luxon": "^3.2.0",
    "@types/object-hash": "^3.0.2",
    "@types/prettier": "^2.7.2",
    "@types/sharp": "^0.31.1",
    "@types/source-map-support": "^0.5.6",
    "@types/yargs-parser": "^21",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^8.0.1",
    "discord-api-types": "0.37.37",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.14",
    "eslint": "latest",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "prettier": "^2.8.7",
    "prisma": "4.12.0",
    "typescript": "5.0.2",
    "yargs-parser": "^21.1.1"
  }
}
