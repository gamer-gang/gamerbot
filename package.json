{
  "name": "gamerbot",
  "version": "2.3.0",
  "license": "MIT",
  "type": "module",
  "packageManager": "yarn@3.2.0",
  "private": true,
  "workspaces": [
    "types"
  ],
  "scripts": {
    "build": "NODE_ENV=production webpack",
    "watch": "NODE_ENV=development webpack watch",
    "dev": "NODE_ENV=development nodemon -r source-map-support/register ./dist/main.mjs",
    "prod": "NODE_ENV=production node ./dist/main.mjs",
    "docker": "docker-compose up --no-log-prefix --attach app",
    "db": "docker-compose up db --no-log-prefix",
    "docgen": "node -r source-map-support/register ./dist/docgen.mjs",
    "types": "tsc && sed -i 's/\"src/\"gamerbot\\/src/g' ./types/gamerbot.d.ts",
    "precommit": "yarn build && yarn docgen && yarn types"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "standard": {
    "engine": "ts-standard"
  },
  "ts-standard": {
    "project": "./tsconfig.json"
  },
  "resolutions": {
    "node-fetch": "^2.6.7"
  },
  "dependencies": {
    "@napi-rs/canvas": "^0.1.20",
    "@prisma/client": "3.10.0",
    "@vvo/tzdb": "^6.47.0",
    "@wiisportsresorts/cowsay": "^0.2.0",
    "axios": "^0.26.0",
    "common-tags": "^1.8.2",
    "didyoumean": "^1.2.2",
    "discord.js": "^13.6.0",
    "emoji-regex": "^10.0.0",
    "he": "^1.2.0",
    "hypixel-cache": "https://github.com/wiisportsresort/hypixel-cache#a41080bde6cd365f8b063cbe60ac48e020d72f57",
    "hypixel-types": "https://github.com/wiisportsresort/hypixel-types#839190e9cbffeb7bb846fd5e414f36a1554ead4c",
    "js-yaml": "^4.1.0",
    "lodash": "^4.17.21",
    "log4js": "^6.4.1",
    "luxon": "^2.3.1",
    "mathjax-full": "^3.2.0",
    "mathjs": "^10.1.1",
    "nodemon": "^2.0.15",
    "object-hash": "^3.0.0",
    "piston-client": "^1.0.2",
    "regenerator-runtime": "^0.13.9",
    "sharp": "^0.30.5",
    "source-map-loader": "^3.0.1",
    "source-map-support": "^0.5.21",
    "string-length": "^5.0.1",
    "tzdata": "^1.0.29",
    "unicode": "^14.0.0"
  },
  "devDependencies": {
    "@swc/core": "^1.2.145",
    "@types/common-tags": "^1",
    "@types/didyoumean": "^1",
    "@types/dts-bundle": "^0",
    "@types/he": "^1",
    "@types/js-yaml": "^4",
    "@types/lodash": "^4",
    "@types/luxon": "^2",
    "@types/object-hash": "^2",
    "@types/prettier": "^2",
    "@types/sharp": "^0",
    "@types/source-map-support": "^0",
    "@types/webpack-node-externals": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "latest",
    "@typescript-eslint/parser": "latest",
    "dotenv": "^16.0.0",
    "eslint": "latest",
    "eslint-config-standard-with-typescript": "^21.0.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.2.0",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "prettier": "^2.5.1",
    "prisma": "3.10.0",
    "swc-loader": "^0.1.15",
    "typescript": "^4.6.1-rc",
    "webpack": "^5.69.1",
    "webpack-cli": "^4.9.2",
    "webpack-node-externals": "^3.0.0"
  }
}
